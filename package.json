{
  "name": "origami-registry-ui",
  "version": "0.0.0",
  "private": true,
  "description": "Get information about Origami components, services, and repositories",
  "keywords": [
    "origami"
  ],
  "license": "MIT",
  "homepage": "https://registry.origami.ft.com/",
  "bugs": {
    "url": "https://github.com/Financial-Times/origami-registry-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Financial-Times/origami-registry-ui.git"
  },
  "engines": {
    "node": "^14.16.1",
    "npm": "^7.11.2"
  },
  "main": "./lib/service.js",
  "scripts": {
    "start": "heroku-node-settings index.js",
    "build:css": "mkdir -p public && sass src/main.scss  --style=compressed --load-path=node_modules --load-path=node_modules/@financial-times --load-path=. | postcss --use autoprefixer --output public/main.css --map",
    "build:js": "mkdir -p public && oax ./src/main.js | swc -f - -C minify > public/main.js",
    "build": "npm-run-all --parallel build:js build:css",
    "watch:css": "nodemon --watch src -e scss -x 'npm run build:css'",
    "watch:js": "nodemon --watch src -e js -x 'npm run build:js'",
    "watch": "npm-run-all --parallel watch:js watch:css",
    "heroku-postbuild": "npm run build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@financial-times/health-check": "2.1.6",
    "@financial-times/o-autoinit": "prerelease",
    "@financial-times/o-buttons": "prerelease",
    "@financial-times/o-colors": "prerelease",
    "@financial-times/o-cookie-message": "prerelease",
    "@financial-times/o-fonts": "prerelease",
    "@financial-times/o-footer-services": "prerelease",
    "@financial-times/o-forms": "prerelease",
    "@financial-times/o-grid": "prerelease",
    "@financial-times/o-header-services": "prerelease",
    "@financial-times/o-icons": "prerelease",
    "@financial-times/o-labels": "prerelease",
    "@financial-times/o-layout": "prerelease",
    "@financial-times/o-loading": "prerelease",
    "@financial-times/o-message": "prerelease",
    "@financial-times/o-normalise": "prerelease",
    "@financial-times/o-spacing": "prerelease",
    "@financial-times/o-syntax-highlight": "prerelease",
    "@financial-times/o-table": "prerelease",
    "@financial-times/o-tabs": "prerelease",
    "@financial-times/o-tracking": "prerelease",
    "@financial-times/o-typography": "prerelease",
    "@financial-times/origami-repo-data-client": "1.6.3",
    "@financial-times/origami-service": "5.0.0",
    "@financial-times/origami-service-makefile": "7.0.3",
    "cheerio": "1.0.0-rc.3",
    "dotenv": "8.2.0",
    "guestlist": "2.3.0",
    "heroku-node-settings": "1.1.0",
    "http-errors": "1.8.0",
    "prismjs": "1.23.0",
    "request-promise-native": "1.0.9",
    "require-all": "3.0.0",
    "sassdoc-extras": "3.0.0",
    "showdown": "1.9.1",
    "snyk": "^1.518.0",
    "throng": "5.0.0"
  },
  "devDependencies": {
    "@financial-times/grafana-tools": "2.0.1",
    "@financial-times/sass": "^1.32.4",
    "@swc/cli": "^0.1.39",
    "@swc/core": "^1.2.54",
    "autoprefixer": "^10.2.5",
    "cssnano": "^5.0.2",
    "eslint": "^7.25.0",
    "eslint-config-origami-component": "^2.1.1",
    "jsdom": "16.5.3",
    "mocha": "8.3.2",
    "mockery": "2.1.0",
    "nodemon": "2.0.7",
    "npm-run-all": "^4.1.5",
    "nyc": "15.1.0",
    "oax": "^0.5.32",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "proclaim": "3.6.0",
    "sass": "^1.32.12",
    "sinon": "10.0.1",
    "supertest": "6.1.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11"
  ],
  "snyk": true,
  "volta": {
    "node": "14.16.1",
    "npm": "7.11.2"
  }
}
